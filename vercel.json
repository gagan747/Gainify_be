{
 "version": 2,
 "builds": [
  {
   "src": "index.js",
   "use": "@vercel/node"
  }
 ],
 "routes": [
  {
   "src": "/api/signup",
   "dest": "/index.js"
  },
  {
   "src": "/api/login",
   "dest": "/index.js"
  },
  {
   "src": "/api/details",
   "dest": "/index.js"
  },
  {
   "src": "/(.*)",
   "dest": "/index.js"
  }
 ]
}